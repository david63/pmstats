{
	"name": "david63/pmstats",
	"type": "phpbb-extension",
	"description": "Adds an Admin option to display a list of the number of member's PM's in each box.",
	"version": "3.1.1-RC1",
	"time": "2020-07-19",
	"keywords": [
		"phpbb",
		"extension",
		"ACP",
		"email"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		}
	],
	"require": {
        "php": ">=7.1.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "PM statistics",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
    	},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "pm_stats_version_file.json",
			"ssl": false
    	}
	}
}
